(defun explode (sym)
  (map 'list #'(lambda (c)
                 (intern (make-string 1
                                      :initial-element c)))
                 (symbol-name sym)))

(defmacro iterate-over-text (text body)
  `(with-open-file (stream ,text)
     (loop for line = (read-line txt nil :eof)
           until (eq line :eof) do
           ,@body)))
            
(defun number? (x)
  (equal 'fixnum (type-of x)))

